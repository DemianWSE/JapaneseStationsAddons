	class(JSA0)
//	setcargotriggers(PASS)
//	flags(FOUNDATIONS)
)

spriteblock(
    set(
	sprite(endbottom.png 8 7 01 122 64 -31 -91)
	sprite(endbottom.png 88 7 01 122 64 -51 -101)
	sprite(endbottom.png 168 7 01 122 64 -31 -91)
	sprite(endbottom.png 248 7 01 122 64 -11 -101)
    )
)

